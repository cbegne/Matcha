{
	"extends": "airbnb",
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es6": true,
		"mocha": true
	},
	"plugins": [
		"react",
		"import",
		"jsx-a11y"
	],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [0],
    "import/extensions": [0]
  }
}
